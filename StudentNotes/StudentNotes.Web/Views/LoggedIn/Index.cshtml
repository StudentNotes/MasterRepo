@model StudentNotes.Logic.ViewModels.Home.HomeViewModel

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LoggedInLayout.cshtml";
}

<div id="login-errors">
    @if (Model != null)
    {
        foreach (var error in Model.LoginViewModel.ErrorList)
        {
            <div class="error-code">@error.Value</div>

        }
        foreach (var success in Model.LoginViewModel.SuccessList)
        {
            <div class="success-code">@success.Value</div>
        }
    }
</div>
